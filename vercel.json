{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "npm run build",
            "use": "@now/static-build",
            "config": {
                "distDir": "dist",
                "production": {
                    "buildCommand": "npm run build",
                    "outputDirectory": "dist"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ]
}