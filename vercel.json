{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node",
            "config": {
                "production": {
                    "buildCommand": "npm run build",
                    "outputDirectory": "dist"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "dist/server.js"
        }
    ]
}